			<include name="Regression"></include>
			<include name="Functional"></include>
		</run>
	</groups>

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="SeleniumEasyTest">
		<parameter name="Browser" value="chrome" />

    <classes>
      <class name="testCases.DropdownListSelectMultipleValuesTest"/>
      <class name="testCases.CheckBoxDemoSingleCheckBoxTest"/>
      <class name="testCases.FirstRadioButtonDemoSingleRadioButtonTest"/>
      <class name="testCases.DatePickerCalendarTest"/>
      <class name="testCases.DropdownListPrintAllValuesTest"/>
      <class name="testCases.FirstFormDemoDataSheetTest"/>
      <class name="testCases.FirstFormDemoTestUsingLaunchBrowser"/>
      <class name="testCases.HeadlessFirstFormDemoDataSheetTest"/>
      <class name="testCases.FirstFormDemoUsingLaunchBrowsrStaticDataTest"/>
      <class name="testCases.NaukriHandleMultipleWindowsTest"/>
      <class name="testCases.DropdownListTest"/>
      <class name="testCases.FirstFormDemoClunkyTest"/>
      <class name="testCases.PopupWindowTest"/>
